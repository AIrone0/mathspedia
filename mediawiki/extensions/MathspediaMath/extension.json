{
	"manifest_version": 2,
	"name": "MathspediaMath",
	"version": "1.0.0",
	"author": "Mathspedia Team",
	"url": "https://github.com/mathspedia/mathspedia",
	"description": "Enhanced math features: LaTeX, interactive code blocks, custom formatting",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.41.0"
	},
	"AutoloadClasses": {
		"MathspediaMathHooks": "includes/MathspediaMathHooks.php",
		"SpecialReportSource": "includes/SpecialReportSource.php",
		"MathspediaSourceReporting": "includes/MathspediaSourceReporting.php"
	},
	"SpecialPages": {
		"ReportSource": "SpecialReportSource"
	},
	"Hooks": {
		"ParserFirstCallInit": "MathspediaMathHooks::onParserFirstCallInit",
		"BeforePageDisplay": "MathspediaMathHooks::onBeforePageDisplay"
	},
	"ResourceModules": {
		"ext.mathspediaMath": {
			"scripts": ["js/mathspedia-math.js"],
			"styles": ["css/mathspedia-math.css"]
		}
	},
	"MessagesDirs": {
		"MathspediaMath": [
			"i18n"
		]
	}
}

